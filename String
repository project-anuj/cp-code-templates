______________________________________________________________________________
                  Sliding Window string matching technique 
______________________________________________________________________________
-----------------------------------------------
|Text| = N;
|Pat| = M;
Total windows to match     : N-M+1
Matching time for 1 window : M
Time complexity            : N*M - M*M + M
for large text and small pattern   : O(N*M)
-----------------------------------------------


#include<bits/stdc++.h>
using namespace std;

string text,pat;
int Lt,Lp;

bool match(int L)
{
	for(int i=0;i<Lp;i++)
	if(text[L+i]!=pat[i])
	return false;
	return true;
}
int main()
{
	int t;
	cin>>t;
	cin.ignore();
	while(t--)
	{
		getline(cin,text);
		getline(cin,pat);
		Lt = text.length();
		Lp = pat.length();
		
		for(int L=0,R=L+Lp-1;R<Lt;L++,R++)
		if(match(L)==true)
		cout<<L<<" ";
		cout<<endl;
	}
}

